{
    "name": "logdo/logdo-laravel",
    "description": "Logdo is a self hosted application logging server that brings back the fun in reading logs. This is the PHP SDK.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mwakaambrose",
            "email": "ambrosemwaka@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {"Logdo\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "Logdo\\Tests\\": "tests/"
        }
    },
    "require": {
        "php":">=7.2",
        "illuminate/support": "^5.5.0",
        "logdo/logdo-php": "dev-master"
    },
    "require-dev": {},
    "extra": {
        "laravel": {
            "providers": [
                "Logdo\\LogdoServiceProvider"
            ]
        }
    }
}
